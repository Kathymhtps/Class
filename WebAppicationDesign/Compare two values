<?php


function compareValues($value1, $value2) {
    if ($value1 > $value2) {
        return "$value1 is greater than $value2";
    } elseif ($value1 < $value2) {
        return "$value1 is less than $value2";
    } else {
        return "$value1 is equal to $value2";
    }
}

// Example values
$value1 = 10;
$value2 = 5;


$result = compareValues($value1, $value2);
echo $result;

?>
